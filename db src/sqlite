CREATE table Data (
	Id BIGINT primary key,
	Sync BOOLEAN,
	Timestamp DATETIME,
	FurTemp REAL,
	FurInper REAL,
	LphTemp REAL,
	LphPress REAL,
	LphInpress REAL,
	LphInflow REAL,
	DeaTemp REAL,
	DeaPress REAL,
	HphTemp REAL,
	HphPress REAL,
	HphInpress REAL,
	HphInflow REAL,
	BoiTemp REAL,
	BoiPress REAL,
	ConTemp REAL,
	ConInflow REAL,
	TurHtemp REAL,
	TurItemp REAL,
	TurLtemp REAL,
	TurHpress REAL,
	TurIpress REAL,
	TurLpress REAL,
	TurSpeed REAL,
);

alter table Data add column PconMode TINYINT;
alter table Data add column PconRun BOOLEAN;
